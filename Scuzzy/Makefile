ALL_LDFLAGS += $(LDFLAGS) -L../thirdparty/SDL2/lib/x64 -L../thirdparty/SDL2_image/lib/x64 -L../thirdparty/SDL2_ttf/lib/x64 -L/usr/lib/x86_64-linux-gnu -L/usr/local/lib -L/usr/lib64 -m64

# Configurations
# #############################################

ifeq ($(origin CC), default)
  CC = gcc
endif
ifeq ($(origin CXX), default)
  CXX = g++
endif
ifeq ($(origin AR), default)
  AR = ar
endif
RESCOMP = windres
INCLUDES += -I. -I../thirdparty/SDL2/include -I../thirdparty/SDL2_image/include -I../thirdparty/SDL2_ttf/include -I/usr/include/SDL2 -I/usr/local/include/SDL2
FORCE_INCLUDE +=
ALL_CPPFLAGS += $(CPPFLAGS) -MD -MP $(DEFINES) $(INCLUDES)
ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
LIBS += -lSDL2main -lSDL2 -lSDL2_image -lSDL2_ttf
LDDEPS +=
LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
define PREBUILDCMDS
endef
define PRELINKCMDS
endef

ifeq ($(config),debug)
TARGETDIR = ../bin/Debug
TARGET = $(TARGETDIR)/Scuzzy
OBJDIR = ../bin-int/Debug
DEFINES += -DPLATFORM_LINUX -DDEBUG
